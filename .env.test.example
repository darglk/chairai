# Zmienne środowiskowe dla testów E2E
# Skopiuj ten plik do .env.test i dostosuj wartości

# URL aplikacji testowej
BASE_URL=http://localhost:4321

# Dane testowego klienta
TEST_CLIENT_EMAIL=test.client@example.com
TEST_CLIENT_PASSWORD=TestPassword123!@#

# Dane testowego rzemieślnika
TEST_ARTISAN_EMAIL=test.artisan@example.com
TEST_ARTISAN_PASSWORD=TestPassword123!@#
TEST_ARTISAN_NIP=1234567890

# Supabase (opcjonalnie, jeśli używasz dedykowanej instancji testowej)
# SUPABASE_URL=https://your-test-project.supabase.co
# SUPABASE_ANON_KEY=your-anon-key

# Timeout dla testów (ms)
TEST_TIMEOUT=30000

# Tryb headless (true/false)
HEADLESS=true

# Liczba workerów dla testów
WORKERS=4
